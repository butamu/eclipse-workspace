
public class Main {

	public static void main(String[] args) {
		Graph graph = new Graph();
		graph.addVertex('A');//0
		graph.addVertex('B');//1
		graph.addVertex('C');//2
		graph.addVertex('D');//3
		graph.addVertex('E');//4
		graph.addVertex('F');//5
		graph.addVertex('G');//6
		graph.addVertex('H');//7
		graph.addVertex('I');//8
		graph.addVertex('J');//9
		graph.addVertex('K');//10
		graph.addVertex('L');//11
		graph.addVertex('M');//12
		
		
		graph.addEdge(1,4);
		graph.addEdge(3,4);
		graph.addEdge(1,2);
		graph.addEdge(0,6);
		graph.addEdge(8,4);
		graph.addEdge(9,10);
		graph.addEdge(3,5);
		graph.addEdge(12,10);
		graph.addEdge(8,12);
		graph.addEdge(0,4);
	
		
		graph.addEdge(0,1);
		graph.addEdge(1,2);
		graph.addEdge(3,4);
		graph.addEdge(0,3);
		
		graph.DFS('A');
	}

}
